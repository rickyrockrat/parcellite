#: ../src/preferences.c:466
msgid "<b>Clipboards</b>"
msgstr "<b>Porta-retalls</b>"

#: ../src/preferences.c:473
msgid "Use _Copy (Ctrl-C)"
msgstr "Usa l'acció de c_opiar (Ctrl-C)"

#: ../src/preferences.c:476
msgid "Use _Primary (Selection)"
msgstr "Usa la se_lecció primària"

#: ../src/preferences.c:479
msgid "S_ynchronize clipboards"
msgstr "S_incronitza els porta-retalls"

#: ../src/preferences.c:487
msgid "<b>History</b>"
msgstr "<b>Historial</b>"

#: ../src/preferences.c:494
msgid "_Save history"
msgstr "De_sa l'historial"

#: ../src/preferences.c:495
msgid "Save and restore history between sessions"
msgstr "Desa i recupera l'historial entre sessions"

#: ../src/preferences.c:500
msgid "_Position history"
msgstr "_Posició de l'historial"

#: ../src/preferences.c:501
msgid "Set the location where history appears. If unchecked, appears where the mouse is."
msgstr "Estableix la ubicació on es mostra l'historial. Si es desactiva, es mostra on sigui ratolí."

#: ../src/preferences.c:505
msgid "<b>X</b>"
msgstr "<b>X</b>"

#: ../src/preferences.c:510
msgid "<b>Y</b>"
msgstr "<b>Y</b>"

#: ../src/preferences.c:519
msgid "Items in history:"
msgstr "Elements a l'historial:"

#: ../src/preferences.c:530
msgid "Max Data Size (MB)"
msgstr "Mida màxima de les dades (MB)"

#: ../src/preferences.c:535
msgid "Set Maximum amount of data to copy for each entry in MBytes. 0 is no limit."
msgstr "Estableix la mida màxima de les dades a copiar per a cada entrada en MBytes. 0 és sense límit."

#: ../src/preferences.c:540
msgid "<b>Miscellaneous</b>"
msgstr "<b>Miscel·lània</b>"

#. *type checkbox
#: ../src/preferences.c:548
msgid "Search As You _Type"
msgstr "Cerca _mentre es tecleja"

#: ../src/preferences.c:549
msgid "If checked, does a search-as-you-type. Turns red if not found. Goes to top (Alt-E) line when no chars are entered for search"
msgstr "Si s'activa, cerca mentre es tecleja. Canvia a vermell si no hi ha coincidències. Va a la primera línia (Alt-E) si no hi ha caràcters per cercar."

#. *case checkbox
#: ../src/preferences.c:553
msgid "_Case Sensitive Search"
msgstr "Di_ferencia entre majúscules i minúscules en cercar"

#: ../src/preferences.c:554
msgid "If checked, does case sensitive search"
msgstr "Si s'activa, es diferencia entre majúscules i minúscules en cercar"

#. * ignore_whiteonly checkbox
#: ../src/preferences.c:558
msgid "_Ignore Whitespace Only"
msgstr "_Ignora el text en blanc"

#: ../src/preferences.c:559
msgid "If checked, will ignore any clipboard additions that contain only whitespace."
msgstr "Si s'activa, s'ignoraran les entrades que només continguin espais en blanc."

#. * trim_wspace_begend checkbox
#: ../src/preferences.c:562
msgid "_Trim Whitespace"
msgstr "Su_primeix els espais en blanc"

#: ../src/preferences.c:563
msgid "If checked, will trim whitespace from beginning and end of entry."
msgstr "Si s'activa, se suprimiran els espais en blanc al principi i al final de l'entrada."

#. * trim_newline checkbox
#: ../src/preferences.c:566
msgid "_Trim Newlines"
msgstr "Su_primeix els salts de línia"

#: ../src/preferences.c:567
msgid "If checked, will replace newlines with spaces."
msgstr "Si s'activa, se substituiran els salts de línia per espais en blanc."

#: ../src/preferences.c:570
msgid "Capture _hyperlinks only"
msgstr "C_aptura només els enllaços"

#: ../src/preferences.c:572
msgid "C_onfirm before clearing history"
msgstr "Demana c_onfirmació abans d'esborrar l'historial"

